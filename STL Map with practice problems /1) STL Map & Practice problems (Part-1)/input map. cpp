#include <bits/stdc++.h>
using namespace std;

int main()
{
    map<string, int> id;
    int n;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        string s;
        int d;
        cin >> s >> d;
        id[s] = d;
    }

    // Output the contents of the map
    for (auto &entry : id)
    {
        cout << entry.first << " " << entry.second << endl;
    }

    return 0;
}
